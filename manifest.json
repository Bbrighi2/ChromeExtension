{
    "name": "Hackathon Chrome Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {         
      "default_popup": "popup.html"
    },
     "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["main.js"]
        }
      ], 
      "icons": {
        "16": "/iconMichael.png",
        "48": "/iconMichael.png",
        "128": "/iconMichael.png"
      }
}



  