{
  "name": "Brandon & Jim's Pride Chrome Extension",
  "version": "1.3",
  "manifest_version": 3,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "/rainbow16.png",
    "48": "/rainbow48.png",
    "128": "/rainbow128.png"
  },
  "web_accessible_resources": ["/RainbowSinebow22px50msLtoR.gif"]
}
